<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/btn_materias"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:clickable="true"
        android:src="@drawable/ex1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn_materias"
        android:layout_alignLeft="@+id/btn_materias"
        android:layout_alignRight="@+id/btn_materias"
        android:layout_alignStart="@+id/btn_materias"
        android:layout_below="@+id/btn_materias"
        android:gravity="center"
        android:text="@string/btn_disciplinas"
        android:textAppearance="?android:textAppearanceMedium"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/btn_eventos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/btn_materias"
        android:layout_marginEnd="39dp"
        android:layout_marginRight="44dp"
        android:clickable="true"
        android:src="@drawable/ex6" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn_notas"
        android:layout_alignEnd="@+id/btn_eventos"
        android:layout_alignLeft="@+id/btn_eventos"
        android:layout_alignRight="@+id/btn_eventos"
        android:layout_alignStart="@+id/btn_eventos"
        android:layout_below="@+id/btn_eventos"
        android:gravity="center"
        android:text="@string/btn_eventos"
        android:textAppearance="?android:textAppearanceMedium"
        android:textColor="@android:color/black"
        android:textStyle="bold" />


    <ImageView
        android:id="@+id/btn_notas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView"
        android:layout_marginTop="45dp"
        android:clickable="true"
        android:src="@drawable/ex3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn_notas"
        android:layout_alignLeft="@+id/btn_notas"
        android:layout_alignRight="@+id/btn_notas"
        android:layout_alignStart="@+id/btn_notas"
        android:layout_below="@+id/btn_notas"
        android:gravity="center"
        android:text="Notas"
        android:textAppearance="?android:textAppearanceMedium"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/btn_lembretes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:layout_alignEnd="@+id/textView2"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_alignTop="@+id/btn_notas"
        android:clickable="true"
        android:src="@drawable/ex4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn_horarios"
        android:layout_alignEnd="@+id/btn_lembretes"
        android:layout_alignLeft="@+id/btn_lembretes"
        android:layout_alignRight="@+id/btn_lembretes"
        android:layout_alignStart="@+id/btn_lembretes"
        android:layout_below="@+id/btn_lembretes"
        android:gravity="center"
        android:text="Lembretes"
        android:textAppearance="?android:textAppearanceMedium"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/btn_horarios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="45dp"
        android:clickable="true"
        android:src="@drawable/ex5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn_horarios"
        android:layout_alignLeft="@+id/btn_horarios"
        android:layout_alignRight="@+id/btn_horarios"
        android:layout_alignStart="@+id/btn_horarios"
        android:layout_below="@+id/btn_horarios"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="Horários"
        android:textAppearance="?android:textAppearanceMedium"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/btn_calendario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView5"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_alignTop="@+id/btn_horarios"
        android:clickable="true"
        android:src="@drawable/ex2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignEnd="@+id/btn_calendario"
        android:layout_alignLeft="@+id/btn_calendario"
        android:layout_alignRight="@+id/btn_calendario"
        android:layout_alignStart="@+id/btn_calendario"
        android:layout_below="@+id/btn_calendario"
        android:gravity="center"
        android:text="Calendário"
        android:textAppearance="?android:textAppearanceMedium"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

</RelativeLayout>